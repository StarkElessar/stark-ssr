		"express": "^5.1.0",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-router": "^7.9.6",
		"typescript": "^5.9.3"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.7",
		"@types/compression": "^1.8.1",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.6",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.1",
