{
	"name": "stark-ssr",
	"version": "1.0.0",
	"scripts": {
		"start": "node ./.output/server/server.mjs",
		"dev": "vite-node --watch --config vite.server.config.ts src/server/server.ts",
		"build": "npm run build:client && npm run build:server",
