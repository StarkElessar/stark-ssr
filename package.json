{
	"name": "dynamic-routing-2",
	"version": "1.0.0",
	"scripts": {
		"start": "node ./.output/server/server.mjs",
		"dev": "vite-node --watch src/server/server.ts",
		"build": "npm run build:client && npm run build:server",
		"build:server": "vite build --config vite.server.config.ts",
		"build:client": "vite build --config vite.client.config.ts",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
		"format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,html,md}'"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"type": "commonjs",
	"dependencies": {
		"@types/express": "^5.0.5",
		"classnames": "^2.5.1",
		"express": "^5.1.0",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-router": "^7.9.6",
		"typescript": "^5.9.3"
	},
	"devDependencies": {
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.6",
		"@types/react-dom": "^19.2.3",
		"@typescript-eslint/eslint-plugin": "^8.47.0",
		"@typescript-eslint/parser": "^8.47.0",
		"@vitejs/plugin-react": "^5.1.1",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"prettier": "^3.6.2",
		"sass": "^1.94.2",
		"vite": "^7.2.4",
		"vite-node": "^3.0.0",
		"vite-plugin-node": "^7.0.0"
	}
}
